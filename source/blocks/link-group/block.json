{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "amnesty-core/link-group",
  "title": "Link Block",
  "category": "amnesty-core",
  "description": "A collection of links, used for things such as further reading suggestions.",
  "keywords": ["Links", "Further Reading"],
  "icon": "list-view",
  "textdomain": "amnesty",
  "editorScript": "file:./index.js",
  "editorStyle": ["amnesty-link-group-editor-style"],
  "style": ["amnesty-link-group-style"],
  "render": "file:./render.php",
  "supports": {
    "className": true,
    "align": ["left", "center", "right"]
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "items": {
      "type": "array",
      "default": []
    }
  }
}
